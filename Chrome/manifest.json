{
  "manifest_version": 2,

  "name": "quick-link",
  "description": "Review the next project for the SDS in one click!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Review the next project!"
  },
  "permissions": [
    "https://api.scratch.mit.edu/",
    "http://api.scratch.mit.edu/",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
